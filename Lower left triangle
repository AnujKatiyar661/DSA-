public class Main
    {            
        static int i,j,k;
                                                     //LOWER_LEFT_TRIANGE
        static void llt(int n)
        {
            for(i=1;i<=n;i++)
            {
                for( j=1;j<=i;j++)
               { System.out.print("*");}
               System.out.print("\n");
            }
        }
        public static void main(String args[])
        {
            int n=5;
             llt(n);
        }
     }
